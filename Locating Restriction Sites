def reverse_complement(s):
    complements = {'A':'T', 'T':'A', 'G':'C', 'C':'G'}
    return ''.join([complements[c] for c in reversed(s)])


def reverse_palindromes(s):
    results = []

    l = len(s)

    for i in range(l):
        for j in range(4, 14):

            if i + j > l:
                continue

            s1 = s[i:i+j]
            s2 = reverse_complement(s1)

            if s1 == s2:
                results.append((i + 1, j))

    return results


if __name__ == "__main__":

    inputFile = open('revp.txt').read().split('\n')

    inputFile.pop(0)

    inputFile = ''.join(inputFile)

    results = reverse_palindromes(inputFile)

    print ("\n".join([' '.join(map(str, r)) for r in results]))
